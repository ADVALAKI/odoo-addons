<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    <!--
		===========================================================
		 TRAINING.COURSENUM 
		===========================================================
		-->
		<!--  FORM -->
		<record model="ir.ui.view" id="training_coursenum_form_view">
			<field name="name">training.coursenum.form.view</field>
			<field name="model">training.coursenum</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Coursenum">
					<group colspan="4">
						<field name="code" />
						<field name="name" />
					</group>
				</form>
			</field>
		</record>
		
		<!--  SEARCH -->
		<record model="ir.ui.view" id="training_coursenum_search_view">
			<field name="name">training.coursenum.tree.view</field>
			<field name="model">training.coursenum</field>
			<field name="type">search</field>
			<field name="arch" type="xml">
				<search string="Coursenum">
					<field name="code" />
					<field name="name" />
				</search>
			</field>
		</record>
		
		<!--  TREE  -->
		<record model="ir.ui.view" id="training_coursenum_tree_view">
			<field name="name">training.coursenum.tree.view</field>
			<field name="model">training.coursenum</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="coursenum">
					<field name="code"/>
					<field name="name" />
				</tree>
			</field>
		</record>
		
		<!-- ACCIONES -->
		<record model="ir.actions.act_window" id="action_coursenum_management_view">
			<field name="name">Coursenum</field>
			<field name="res_model">training.coursenum</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
		<!-- /ACCIONES -->
		
		<!-- SUBMENU -->
		<menuitem id="coursenum_menu_view" name="Coursenum"
			parent="training.training_library_mi" action="action_coursenum_management_view" />
    
    </data>
</openerp>