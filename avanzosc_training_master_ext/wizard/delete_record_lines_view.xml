<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record model="ir.ui.view" id="wiz_delete_record_lines_form_view">
			<field name="name">wiz.delete.record.lines.form</field>
			<field name="model">wiz.delete.record.lines</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Delete Lines">
					<group colspan="4">
						<label string="Are you sure, you want to delete no used Record lines?"/>
					</group>
					<group colspan="4">
						<button icon='gtk-cancel' special="cancel" string="Cancel" />
						<button name="delete_lines" icon='gtk-ok' type="object"
							string="Delete" />
					</group>
				</form>
			</field>
		</record>
		
		<act_window name="Delete Lines" res_model="wiz.delete.record.lines"
		src_model="training.record" view_mode="form" target="new" key2="client_action_multi"
		id="act_wiz_delete_record_lines" />

	</data>
</openerp>