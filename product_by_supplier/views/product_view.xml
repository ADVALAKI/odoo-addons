<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
            <record id="product_search_form_inh" model="ir.ui.view">
            <field name="name">product.search.form.inh</field>
            <field name="model">product.product</field>
            <field name="type">search</field>
            <field name="inherit_id" ref="product.product_search_form_view" />
            <field name="arch" type="xml">
                <filter string="Products" position="after">
                    <filter string="No Seller" icon="terp-accessories-archiver"
                    domain="[('seller_ids','=',False)]"/>
                </filter>
            </field>
        </record>

    </data>
</openerp>