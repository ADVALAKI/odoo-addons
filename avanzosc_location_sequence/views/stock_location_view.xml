<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="view_location_tree_inh" model="ir.ui.view">
			<field name="name">view.location.tree.inh</field>
			<field name="model">stock.location</field>
			<field name="type">tree</field>
			<field name="inherit_id" ref="stock.view_location_tree2" />
			<field name="arch" type="xml">
				<field name="complete_name" position="before">
					<field name="alpha_loc" />
				</field>
			</field>
		</record>

		<record id="view_location_form_inh" model="ir.ui.view">
			<field name="name">view.location.form.inh</field>
			<field name="model">stock.location</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="stock.view_location_form" />
			<field name="arch" type="xml">
				<data>
					<field name="name" position="after">
						<field name="alpha_loc" />
					</field>
				</data>
			</field>
		</record>

	</data>
</openerp>
