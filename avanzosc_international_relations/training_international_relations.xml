			<field name="model">scholarship</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Agreements">
					<group colspan="4">
						<field name="name" />
						<field name="init_date" />
						<field name="end_date" />
						<field name="granted_amount" />
						<field name="scholarship_type_id" />
						<field name="scholarship_mode" />
					</group>
					<notebook colspan="4">
						<page string="Student studies">
							<field name="scholarship_line_student_ids" nolabel="1"/>
						</page> 
						<page string="Practices">
							<field name="scholarship_line_practice_ids" nolabel="1"/>
