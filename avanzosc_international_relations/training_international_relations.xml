						<button name="button_refuse" states="draft" string="Refuse" icon="gtk-ok"/>
						<button name="button_complete" states="accepted" string="Complete" icon="gtk-ok"/>
						<button name="button_draft" states="completed,refused" string="Draft" icon="gtk-ok"/>
					</group>
				</form>
			</field>
