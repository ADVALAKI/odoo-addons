<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

	<!--
	===========================================================
	 	AGREEMENT
	===========================================================
	-->		

		<!-- TREE -->

		<record model="ir.ui.view" id="agreement_tree_view">
			<field name="name">agreement.tree.view</field>
			<field name="model">agreement</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Agreements">
					<field name="registration_number" />
					<field name="institution_id"/>
					<field name="institution_type" />
					<field name="agreement_date" />
					<field name="erasmus"/>
					<field name="erasmus_code"/>
				</tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="agreement_search_view">
			<field name="name">agreement.search.view</field>
			<field name="model">agreement</field>
			<field name="type">search</field>
			<field name="arch" type="xml">
				<search string="Agreements">
					<field name="registration_number" />
					<field name="institution_id"/>
					<field name="institution_type" />
					<field name="agreement_date" />
					<field name="erasmus"/>
					<field name="erasmus_code"/>
				</search>
			</field>
		</record>

		<!-- FORM -->

		<record model="ir.ui.view" id="agreement_form_view">
			<field name="name">agreement.form.view</field>
			<field name="model">agreement</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Agreements">
					<group colspan="4">
						<field name="registration_number" />
						<field name="institution_id" domain="[('institution', '=', True)]"/>
						<field name="institution_type" />
					</group>
					<group colspan="2">
						<field name="still_stands" />
						<field name="not_working" />
						<field name="practical_effect" />
						<field name="extensions" />
					</group>
					<group colspan="2">
						<field name="subject1" />
						<field name="subject2" />
						<field name="subject3" />
						<field name="subject4" />
						<field name="subject5" />
					</group>
					<group colspan="2">
						<field name="erasmus" />
						<field name="erasmus_code" attrs="{'invisible':[('erasmus','=',False)]}"/>
					</group>
					<field name="agreement_date" />
					<notebook colspan="4">
						<page string="Offers">
							<field name="offer_ids" nolabel="1"/>
						</page>
						<page string="Summary">
							<field name="summary" nolabel="1"/>
						</page>
						<page string="Comments">
							<field name="comments" nolabel="1"/>
						</page>
					</notebook>
				</form>
			</field>
		</record>
	
