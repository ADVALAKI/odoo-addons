					</notebook>
					<group colspan="2">
						<field name="state" />
					</group>
					<group colspan="2">
						<button name="button_active" states="draft" string="Active" icon="gtk-ok"/>
						<button name="button_close" states="active" string="Close" icon="gtk-ok"/>
						<button name="button_reactive" states="closed" string="Reactive" icon="gtk-ok"/>
					</group>
				</form>
			</field>
		</record>
	
