<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="birthdate_date_form_view_inh_student_course">
            <field name="name">birthdate.date.form.view.inh.student.course</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="partner_contact_birthdate.birthdate_date"/>
            <field name="arch" type="xml">
                <field name="birthdate_date" position="after">
                    <newline/>
                    <separator string="Course information" colspan="4"/>
                    <field name="student_age" />
                    <field name="student_repeated_courses" />
                    <field name="student_course" />
                    <field name="student_class" />
                    <field name="student_rockbotic_level" />
                </field>
            </field>
        </record>
    </data>
</openerp>
